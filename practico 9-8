#include <iostream>
#include <cmath>
using namespace std;

void ECUACION(float a, float b, float c) {
    float d, x1, x2, real, imag;
    d = b * b - 4 * a * c;

    if (a == 0)
        cout << "No es una ecuacion cuadratica." << endl;
    else if (d > 0) {
        x1 = (-b + sqrt(d)) / (2 * a);
        x2 = (-b - sqrt(d)) / (2 * a);
        cout << "Raices reales: " << x1 << " y " << x2 << endl;
    }
    else if (d == 0) {
        x1 = -b / (2 * a);
        cout << "Raiz doble: " << x1 << endl;
    }
    else {
        real = -b / (2 * a);
        imag = sqrt(-d) / (2 * a);
        cout << "Raices complejas: " << real << " + " << imag << "i y "
             << real << " - " << imag << "i" << endl;
    }
}

int main() {
    float a, b, c;
    cout << "Ingrese a, b y c: ";
    cin >> a >> b >> c;
    ECUACION(a, b, c);
    return 0;
}
