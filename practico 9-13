#include <iostream>
#include <cmath>
using namespace std;

float REDONDEAR(float r, int n) {
    float factor = pow(10, n);
    r = round(r * factor) / factor;
    return r;
}

int main() {
    float r;
    int n;
    cout << "Ingrese un numero real: ";
    cin >> r;
    cout << "Ingrese cantidad de decimales: ";
    cin >> n;

    cout << "Numero redondeado: " << REDONDEAR(r, n) << endl;

    return 0;
}
