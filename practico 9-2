#include <iostream>
using namespace std;

int FAC(int n) {
    int f = 1;
    for (int i = 1; i <= n; i++) {
        f = f * i;
    }
    return f;
}

int main() {
    int num, resultado;

    cout << "Ingrese un numero entero: ";
    cin >> num;

    resultado = FAC(num);

    cout << "El factorial de " << num << " es: " << resultado << endl;

    return 0;
}

